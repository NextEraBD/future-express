    <record model="crm.stage" id="stage_lead_333">
        <field name="name">Ready To Quote</field>
        <field name="is_ready">True</field>
        <field name="sequence">6</field>
        <field name="order">6</field>
    </record>
    <record model="crm.stage" id="stage_lead_6">
        <field name="name">Negotiate</field>
        <field name="sequence">7</field>
