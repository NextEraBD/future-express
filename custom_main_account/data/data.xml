<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Debit Memo -->
        <record id="seq_partner_order" model="ir.sequence">
            <field name="name">Partner Code</field>
            <field name="code">res.partner.seq</field>
            <field name="prefix">P</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="debit_memo_seq" model="ir.sequence">
            <field name="name">Debit Memo</field>
            <field name="code">debit.memo</field>
            <field name="padding">4</field>
            <field name="prefix">Debit/Memo/</field>
        </record>

        <record id="credit_memo_seq" model="ir.sequence">
             <field name="name">Credit Memo</field>
            <field name="code">credit.memo</field>
            <field name="padding">4</field>
            <field name="prefix">Credit/Memo/</field>
        </record>

    </data>
</odoo>
