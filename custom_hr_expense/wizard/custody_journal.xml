        <record id="custody_custody_wizard_view_form" model="ir.ui.view">
            <field name="name">custody.custody.wizard.form</field>
            <field name="model">custody.custody.wizard</field>
            <field name="arch" type="xml">
                <form string="Create Entry">
                    <sheet>
                        <group>
                            <field name="journal_id" />
<!--                            <field name="branch_id" />-->
