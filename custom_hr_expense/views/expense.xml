                <p class="o_view_nocontent_smiling_face">
                    Create a new expense service
                </p>
            </field>
      </record>


    <menuitem id="menu_expense_service_type"  name="Expense Type" parent="hr_expense.menu_hr_expense_configuration" action="expense_type_action"/>
